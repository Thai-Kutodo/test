                <g>
                  <text class="mycard__name" x="30" y="47">{{ customer.last_name }} {{ customer.first_name }} 様</text>
                  {% if customer.metafields.smaregi.customer_code.value %}
                    <text class="mycard__number" x="30" y="65">No. {{ customer.metafields.smaregi.customer_code.value | truncate: 7, ""}}</text>
                  {% else %}
                    <text class="mycard__number" x="30" y="65">No. 0000000 </text>
                  {% endif %}
                  <text class="mycard__point" x="30" y="107">現在保有fポイント</text>
                  {% assign useablePoints = 0 %}
                  {% if customer.metafields.loyalty.easy_points_attributes.value["balance"] %}
                    {% assign points = customer.metafields.loyalty.easy_points_attributes.value["balance"] %}
                    {% assign roundedPoints = points | divided_by: 1000 | floor %}
                    {% if roundedPoints > 0 %}
                     {% assign useablePoints = roundedPoints | times: 1000 %}
                    {% endif %}
                    <text class="mycard__point-num" x="244" y="109">{{ points | times: 100 | money | replace: "¥" : "" | repalce: "円" | "" }}pt</text>
                  {% else %}
                    <text class="mycard__point-num" x="244" y="109">0pt</text>
                  {% endif %}
                  <text class="mycard__point" x="30" y="133">現在利用可能なfポイント</text>
                  {% if customer.metafields.loyalty.easy_points_attributes.value["balance"] %}
                    <text class="mycard__point-num" x="244" y="135">{{ useablePoints | times: 100 | money | replace: "¥" : "" | replace: "円" | "" }}pt</text>
                  {% else %}
                    <text class="mycard__point-num" x="244" y="135">0pt</text>
                  {% endif %}s
                  <text class="mycard__point-expiration" x="30" y="155">fポイント有効期限 {{ customer.metafields.loyalty.easy_points_attributes.value["expiration_date"] | date: "%Y/%m/%d"  }}</text>
                </g>
              </svg>
              {% section 'customer-barcode' %}
              
              {% comment %}
              <div class="mycard_barcode_container">
                <svg id="barcode"></svg>
                  {%- assign barcodeNum = customer.metafields.smaregi.customer_code.value -%}
                  {%- if barcodeNum -%}
                    <script>
                      JsBarcode("#barcode", {{ barcodeNum }}, { height: 16, displayValue: false});
                    </script>
                  {%- endif -%}
                </div>
              {% endcomment %}
